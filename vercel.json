{
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/en-gb/data/:path*",
      "destination": "/data/en-gb/:path*"
    },
    {
      "source": "/en-gb/",
      "destination": "/index.html"
    },
    {
      "source": "/en-gb/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/en-us/data/:path*",
      "destination": "/data/en-us/:path*"
    },
    {
      "source": "/en-us/",
      "destination": "/index.html"
    },
    {
      "source": "/en-us/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/tugs",
      "destination": "/index.html"
    },
    {
      "source": "/tugs/:path*",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/games/the-build-engine-game/The_Build_Engine_Game.swf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-shockwave-flash"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ]
}

