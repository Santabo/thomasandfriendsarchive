{
  "redirects": [
    {
      "source": "/",
      "destination": "/en-gb",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/en-gb/data/:path*",
      "destination": "/data/en-gb/:path*"
    },
    {
      "source": "/en-gb",
      "destination": "/index.html"
    },
    {
      "source": "/en-gb/",
      "destination": "/index.html"
    },
    {
      "source": "/en-gb/:path*",
      "has": [{ "type": "missing", "key": "filesystem" }],
      "destination": "/index.html"
    }
  ]
}
